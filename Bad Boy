#include<bits/stdc++.h>
using namespace std;
#define fast              ios::sync_with_stdio(false);cin.tie(0);
#define ll                long long


void result()
{
   int n,m,i,j;
   cin>>n>>m>>i>>j;
 
   if(i<=n/2 && j<=m/2)
    cout<<"1 "<<m<<" "<<n<<" 1"<<endl;
    
    else if(i>=n/2 && j>=m/2)
    cout<<"1 "<<m<<" "<<n<<" 1"<<endl;
    else
    cout<<"1 1 "<<n<<" "<<m<<endl; 
 }
 
 int main()
{
    fast
    
    ll t;cin>>t;
    while(t--)
    {
        result();
    }
    
    return 0;
}
